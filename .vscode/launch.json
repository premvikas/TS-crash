{
    "version": "0.2.0",
    "configurations": [
            {
                "name": "Current TS File",
                "type": "node",
                "request": "launch",
                "args": ["${relativeFile}"],
                "runtimeArgs": ["--nolazy", "-r", "ts-node/register"],
                "sourceMaps": true,
                "cwd": "${workspaceFolder}/src/index.ts",
                "protocol": "inspector",
            }
    ]
}